<template name="Calendar"><view class="calendar">
                <view class="e-head">
                    <block wx:for="{{state.week}}" wx:for-item="item" wx:for-index="idx" wx:key="*this"><block>
                                <block><block wx:if="{{idx == 0 || idx == 6}}"><view class="w s">{{item}}</view></block><block wx:else="true"><view class="w">{{item}}</view></block></block>
                            </block></block>
                </view>
                <scroll-view class="m-calendar" scroll-y="true" scroll-into-div="{{state.todivId}}">
                    <block wx:for="{{state.calendarArray}}" wx:for-item="month" wx:for-index="idx" wx:key="*this"><view>
                                <view class="e-month">
                                    <view class="b-header">{{month.date}}</view>
                                    <block wx:for="{{month.daysArray}}" wx:for-item="itemRow" wx:for-index="Row" wx:key="*this"><view class="b-row">
                                                <block wx:for="{{itemRow}}" wx:for-item="item1" wx:for-index="index1" wx:key="*this"><block>
                                                            <block><block wx:if="{{item1.isBlank}}"><view class="item" /></block><block wx:else="true"><view bindtap="dispatchEvent" class="{{'item item-a ' + (item1.isWeekend ? 'weekend' : '')}}" data-tap-uid="e8927" data-class-uid="c5450" data-instance-uid="{{props.instanceUid}}" data-key="{{idx + '-' + Row + '-' + index1}}">
                                                                    <view class="day">{{item1.showDate}}</view>
                                                                </view></block></block>
                                                        </block></block>
                                            </view></block>
                                </view>
                            </view></block>
                </scroll-view>
            </view></template>