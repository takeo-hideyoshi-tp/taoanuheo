<view class="container">
                <view class="page-body">
                    <view class="page-section page-section-spacing swiper">
                        <swiper indicator-dots="{{state.indicatorDots}}" autoplay="{{state.autoplay}}" circular="{{state.circular}}" vertical="{{state.vertical}}" interval="{{state.interval}}" duration="{{state.duration}}" previous-margin="{{state.previousMargin + 'px'}}" next-margin="{{state.nextMargin + 'px'}}">
                            <block wx:for="{{state.background}}" wx:for-item="item" wx:for-index="i1878" wx:key="*this"><swiper-item>
                                        <view class="{{'swiper-item ' + item}}" />
                                    </swiper-item></block>
                        </swiper>
                    </view>
                    <view class="page-section">
                        <view class="weui-cells weui-cells_after-title">
                            <view class="weui-cell weui-cell_switch">
                                <view class="weui-cell__bd">指示点</view>
                                <view class="weui-cell__ft">
                                    <switch checked="{{state.indicatorDots}}" bindchange="dispatchEvent" data-property-name="indicatorDots" data-change-uid="e5536" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                                </view>
                            </view>
                            <view class="weui-cell weui-cell_switch">
                                <view class="weui-cell__bd">自动播放</view>
                                <view class="weui-cell__ft">
                                    <switch checked="{{state.autoplay}}" bindchange="dispatchEvent" data-property-name="autoplay" data-change-uid="e6692" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                                </view>
                            </view>
                            <view class="weui-cell weui-cell_switch">
                                <view class="weui-cell__bd">衔接滑动</view>
                                <view class="weui-cell__ft">
                                    <switch checked="{{state.circular}}" bindchange="dispatchEvent" data-property-name="circular" data-change-uid="e7838" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                                </view>
                            </view>
                            <view class="weui-cell weui-cell_switch">
                                <view class="weui-cell__bd">竖向</view>
                                <view class="weui-cell__ft">
                                    <switch checked="{{state.vertical}}" bindchange="dispatchEvent" data-property-name="vertical" data-change-uid="e8980" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                                </view>
                            </view>
                        </view>
                    </view>

                    <view class="page-section page-section-spacing">
                        <view class="page-section-title">
                            <text>幻灯片切换时长(ms)</text>
                            <text class="info">{{state.duration}}</text>
                        </view>
                        <slider value="{{state.duration}}" min="500" max="2000" bindchange="dispatchEvent" data-property-name="duration" data-change-uid="e10496" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                        <view class="page-section-title">
                            <text>自动播放间隔时长(ms)</text>
                            <text class="info">{{state.interval}}</text>
                        </view>
                        <slider value="{{state.interval}}" min="2000" max="10000" bindchange="dispatchEvent" data-property-name="interval" data-change-uid="e11558" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                        <view class="page-section-title">
                            <text>前边距(px)</text>
                            <text class="info">                                {{state.changeProperty}}                            </text>
                        </view>
                        <slider value="{{state.previousMargin}}" min="0" max="50" bindchange="dispatchEvent" data-property-name="previousMargin" data-change-uid="e12746" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                        <view class="page-section-title">
                            <text>后边距(px)</text>
                            <text class="info">{{state.nextMargin}}</text>
                        </view>
                        <slider value="{{state.nextMargin}}" min="0" max="50" bindchange="dispatchEvent" data-property-name="nextMargin" data-change-uid="e13806" data-class-uid="c7148" data-instance-uid="{{props.instanceUid}}" />
                    </view>
                </view>
            </view>