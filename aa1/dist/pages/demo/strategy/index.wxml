<view class="strategy">
                <block wx:for="{{state.data}}" wx:for-item="item" wx:for-index="index" wx:key="*this"><view bindtap="dispatchEvent" class="strategy-item" data-tap-uid="e2271" data-class-uid="c2128" data-instance-uid="{{props.instanceUid}}" data-key="{{index}}">
                                <image class="big-image" src="{{item.bigImage}}">
                                </image>
                                <view class="strategy-item-content">
                                    <view class="desc">{{item.desc}}</view>
                                    <view class="date">{{item.date}}</view>
                                    <view class="user-wrapper">
                                        <view class="user-image">
                                            <image src="{{item.userImage}}" />
                                        </view>
                                        <view class="user-name">{{item.userName}}</view>
                                    </view>
                                </view>
                            </view></block>
            </view>