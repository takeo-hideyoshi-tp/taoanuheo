{
    "compileOnSave": false,
    "compilerOptions": {
        "lib": ["dom", "es2015.promise", "es5"],
        "jsx": "preserve",
        "target": "es2016",
        "module": "es2015",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": false,
        "preserveConstEnums": true,
        "sourceMap": true,
        "skipLibCheck": true
    }
}