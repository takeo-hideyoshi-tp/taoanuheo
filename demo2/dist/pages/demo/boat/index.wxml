<view class="boat">
                <view class="content">
                    <view class="city-select-container">
                        <view bindtap="dispatchEvent" class="orgin-city-wrapper" data-tap-uid="e3787" data-class-uid="c3160">
                            <view class="tip-wrapper"><view class="dot"></view><text>出发</text></view>
                            <view class="orgin-ctiy">{{state.orginCity}}</view>
                        </view>
                        <view bindtap="dispatchEvent" class="switch-logo" data-tap-uid="e4394" data-class-uid="c3160">
                            <image src="../../../assets/image/switch.png" />
                        </view>
                        <view bindtap="dispatchEvent" class="target-city-wrapper" data-tap-uid="e4790" data-class-uid="c3160">
                            <view class="tip-wrapper"><view class="dot"></view>到达</view>
                            <view class="target-ctiy">{{state.targerCity}}</view>
                        </view>
                    </view>
                    <view bindtap="dispatchEvent" class="date-select-container" data-tap-uid="e5426" data-class-uid="c3160">
                        <view class="title">出发日期</view>
                        <view class="date">{{state.date}}</view>
                    </view>
                    <view bindtap="dispatchEvent" class="search-btn" data-tap-uid="e5901" data-class-uid="c3160">开始查询</view>
                </view>
            </view>